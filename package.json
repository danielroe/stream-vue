{
  "name": "stream-vue",
  "version": "0.3.3",
  "description": "Cloudflare Stream component for VueJS",
  "keywords": [
    "vue",
    "vuejs",
    "cloudflare",
    "video",
    "component",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/stream-vue",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "vetur/**/*",
    "!**/*.map"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm compile",
    "clean": "rm -rf lib/*",
    "compile": "siroc build",
    "lint": "pnpm lint:all:eslint && pnpm lint:all:prettier",
    "lint:all:eslint": "pnpm lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "pnpm lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "husky install && pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "pnpm test:unit",
    "test:unit": "vitest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.22.3",
    "@siroc/eslint-config": "^0.2.0",
    "@siroc/jest-preset": "^0.2.0",
    "@types/jest": "^29.5.2",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.2.5",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitest/coverage-v8": "^0.32.0",
    "@vue/test-utils": "^1.3.6",
    "bumpp": "^9.1.1",
    "conventional-changelog-conventionalcommits": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.8.8",
    "semver": "^7.5.1",
    "siroc": "^0.10.0",
    "typescript": "^4.2.4",
    "vitest": "^0.32.0",
    "vue": "2.7.14",
    "vue-template-compiler": "^2.7.14"
  },
  "packageManager": "pnpm@8.6.1",
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  }
}
